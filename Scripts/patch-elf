#/bin/sh

SONAME="$1"
shift

set -e

# patchelf "$@" "/root/lib/x86_64/$SONAME"
patchelf "$@" "/root/lib/arm64-v8a/$SONAME"
patchelf "$@" "/root/lib/armeabi-v7a/$SONAME"

# cp "/root/lib/x86_64/$SONAME" "/root/swift-5.8-android-24-sdk/usr/lib/x86_64-linux-android/$SONAME"
cp "/root/lib/arm64-v8a/$SONAME" "/root/swift-5.8-android-24-sdk/usr/lib/aarch64-linux-android/$SONAME"
cp "/root/lib/armeabi-v7a/$SONAME" "/root/swift-5.8-android-24-sdk/usr/lib/arm-linux-androideabi/$SONAME"


